<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="
       http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <reference id="xmppMsgService" interface="com.kalix.middleware.xmpp.api.biz.IXmppMsgService"/>

    <!--新计划任务消息-->
    <service interface="org.osgi.service.event.EventHandler" ref="xmppMsgEventImpl">
        <service-properties>
            <entry key="event.topics" value="com/kalix/schedule/message/new"/>
        </service-properties>
    </service>

    <bean id="xmppMsgEventImpl" class="com.kalix.common.message.biz.xmppevent.XmppMsgEventImpl">
        <property name="userBeanService" ref="userBeanService"/>
        <property name="xmppMsgService" ref="xmppMsgService"/>
    </bean>

</blueprint>

